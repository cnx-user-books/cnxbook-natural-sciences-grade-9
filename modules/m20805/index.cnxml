              <entry>FOOD GROUP</entry>
              <entry>Sources</entry>
              <entry>Function</entry>
              <entry>Building units</entry>
            </row>
            <row>
              <entry>PROTEINS</entry>
              <entry>Meat, fish, eggs, soya</entry>
              <entry>Growth and repair</entry>
              <entry>Amino acids</entry>
            </row>
            <row>
              <entry>CARBOHYDRATES</entry>
              <entry>Starches, e.g. bread and pastaSugars, e.g. cane sugar, honey</entry>
